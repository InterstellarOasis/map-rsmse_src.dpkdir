	surfaceparm alphashadow
	surfaceparm detail
	surfaceparm lightfilter
	surfaceparm trans
	sort 9
	cull none
	{
		diffuseMap  textures/rsmse_custom_src/glass1_d
		normalMap   textures/rsmse_custom_src/glass1_n
		specularMap textures/rsmse_custom_src/glass1_s
		blend blend
		alphaGen const 0.5
	}
}

