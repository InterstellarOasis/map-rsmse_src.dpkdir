textures/rsmse/glass1
{
	qer_editorImage textures/rsmse/glass1.tga
	qer_trans 0.5
	surfaceparm alphashadow
	surfaceparm detail
	surfaceparm lightfilter
	surfaceparm trans
	sort 9
	cull none

	{
	stage diffusemap
	map textures/rsmse/glass1.tga
	blend blend
	alphaGen const 0.5
	}
	normalMap textures/rsmse/glass1_norm.tga
	specularMap textures/rsmse/glass1_gloss.tga
	
}

textures/rsmse/lightcone1
{
	qer_editorImage textures/rsmse/lightcone1.tga
	qer_trans 0.25
	surfaceparm detail
	surfaceparm nonsolid
	surfaceparm trans
	sort 10
	cull none

	{
	stage diffusemap
	map textures/rsmse/lightcone1.tga
	blend blend
	alphaGen const 0.5
	rgbGen identity
	tcMod scroll 0.1 0
	}
}

